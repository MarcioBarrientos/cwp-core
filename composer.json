{
    "name": "cwp/cwp-core",
    "description": "CWP basic compatibility module. Use for migrations.",
    "type": "silverstripe-vendormodule",
    "license": "BSD-3-Clause",
    "keywords": [
        "silverstripe",
        "cwp"
    ],
    "require": {
        "silverstripe/cms": "^4",
        "silverstripe/framework": "^4",
        "silverstripe/admin": "^1",
        "silverstripe/assets": "^1",
        "silverstripe/versioned": "^1",
        "silverstripe/hybridsessions": "dev-master as 2.0.x-dev",
        "silverstripe/environmentcheck": "^2@dev",
        "silverstripe/auditor": "^2@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "CWP\\Core\\": "src/",
            "CWP\\Core\\Tests\\": "tests/"
        }
    },
    "extra": {
        "expose": [
            "css",
            "javascript"
        ],
        "branch-alias": {
            "dev-master": "2.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
